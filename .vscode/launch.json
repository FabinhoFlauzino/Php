    "version": "0.2.0",
    "configurations": [
    {
        "name": "PowerShell Launch Current File in Temporary Console",
        "type": "PowerShell",
        "request": "launch",
        "script": "${file}",
        "args": [],
        "cwd": "${file}",
        "createTemporaryIntegratedConsole": true
    },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        }
    ]
}